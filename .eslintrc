// Use this file as a starting point for your project's .eslintrc.
// Copy this file, and add rule overrides as needed.
{
  "extends": "airbnb/base",
  "env": {
    "node": true
  },
  "rules": {
    // disable requiring trailing commas
    "comma-dangle": 0,
    // Allow writing to console for NodeJS
    "no-console": 0,
    "strict": 0
  }
}